{
  "name": "cube-records",
  "version": "0.0.1",
  "description": "Opinionated wrapper around @cubejs-client/react.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cube-records": "dist/cli/index.js"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "dev:cli": "node dist/cli/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "commit": "cz",
    "prepare": "husky"
  },
  "keywords": ["cubejs", "react", "typescript"],
  "author": "Aliou Diallo <code@general-dexterity.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.8.0",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cubejs-client/core": "^1.3.12",
    "@cubejs-client/react": "^1.3.12",
    "@types/node": "^22.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "typescript": "^5.6.0",
    "vitest": "^3.1.3"
  },
  "peerDependencies": {
    "@cubejs-client/core": "^1.3.12",
    "@cubejs-client/react": "^1.3.12"
  },
  "dependencies": {
    "commander": "^14.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
