{
  "name": "@general-dexterity/cube-records",
  "version": "0.6.0",
  "description": "Opinionated wrapper around @cubejs-client/react",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cube-records": "./bin/cube-records.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "prepublishOnly": "npm run build",
    "codegen": "cube-records-codegen"
  },
  "keywords": [
    "cubejs",
    "react",
    "typescript"
  ],
  "author": "Aliou Diallo <code@general-dexterity.com>",
  "license": "MIT",
  "dependencies": {
    "@general-dexterity/cube-records-codegen": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "latest",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.6.0",
    "vitest": "latest"
  },
  "peerDependencies": {
    "@cubejs-client/core": "^1.3.34",
    "@cubejs-client/react": "^1.3.34",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
