{
  "name": "@general-dexterity/cube-records-codegen",
  "version": "0.0.1",
  "description": "CLI tool for generating Cube Record type definitions from a CubeJS server",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cube-records": "dist/index.js"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "dev:cli": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "test": "vitest --run",
    "test:watch": "vitest --watch"
  },
  "keywords": ["cubejs", "react", "typescript", "cli"],
  "author": "Aliou Diallo <code@general-dexterity.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "latest",
    "@cubejs-client/core": "^1.3.12",
    "@cubejs-client/react": "^1.3.12",
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
    "vitest": "latest"
  },
  "peerDependencies": {
    "@cubejs-client/core": "^1.3.12",
    "@cubejs-client/react": "^1.3.12"
  }
}
